programa {
  funcao inicio() {
    real lado, area, dobro_area

    escreva("\nQual a medida do lado do Quadrado: \n")
    leia(lado)

    area = lado*lado
    dobro_area= area*area

    escreva("\nA área do quadrado é ",area, "\n\n")
    escreva("O dobro da Área é ",dobro_area, "\n")
       

  }
}
